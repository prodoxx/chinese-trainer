{
	"name": "chinese-app",
	"version": "0.1.0",
	"private": true,
	"packageManager": "bun@1.2.7",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "node scripts/validate-database-url.js && next start",
		"lint": "next lint",
		"postinstall": "prisma generate",
		"load-dict": "bun run scripts/load-cedict.ts",
		"clean-db": "bun run scripts/clean-database.ts",
		"workers": "bun run src/lib/queue/start-workers.ts",
		"empty-queues": "bun run scripts/empty-queues.ts",
		"migrate-to-r2": "bun run scripts/migrate-to-r2.ts",
		"migrate-to-r2:dry": "bun run scripts/migrate-to-r2.ts --dry-run",
		"db:generate": "prisma generate",
		"db:migrate": "prisma migrate dev",
		"db:migrate:prod": "prisma migrate deploy",
		"db:push": "prisma db push",
		"db:studio": "prisma studio",
		"db:reset": "prisma migrate reset",
		"set-admin": "bun run scripts/set-admin-role.ts"
	},
	"dependencies": {
		"@auth/prisma-adapter": "^2.10.0",
		"@aws-sdk/client-s3": "^3.850.0",
		"@aws-sdk/lib-storage": "^3.850.0",
		"@aws-sdk/s3-request-presigner": "^3.850.0",
		"@fal-ai/client": "^1.6.1",
		"@ffmpeg-installer/ffmpeg": "^1.1.0",
		"@prisma/client": "^6.13.0",
		"bcryptjs": "^3.0.2",
		"bullmq": "^5.56.7",
		"dotenv": "^17.2.1",
		"fluent-ffmpeg": "^2.1.3",
		"framer-motion": "^12.23.12",
		"ioredis": "^5.6.1",
		"lucide-react": "^0.526.0",
		"mongodb": "^6.18.0",
		"mongoose": "^8.16.5",
		"next": "15.4.4",
		"next-auth": "^4.24.11",
		"openai": "^5.10.2",
		"prisma": "^6.13.0",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"recharts": "^3.1.0",
		"resend": "^4.7.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/bcryptjs": "^3.0.0",
		"@types/fluent-ffmpeg": "^2.1.27",
		"@types/mongodb": "^4.0.7",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@types/xml2js": "^0.4.14",
		"eslint": "^9",
		"eslint-config-next": "15.4.4",
		"postcss-import": "^16.1.1",
		"tailwindcss": "^4",
		"typescript": "^5",
		"xml2js": "^0.6.2"
	}
}
